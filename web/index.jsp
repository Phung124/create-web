    <!-- Featured Section Begin -->
    <section class="featured spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title">
                        <h2>Featured Product</h2>
                    </div>
                    <div class="featured__controls">
                         
                        <ul>
                            
                            <li><a href="category?did=${o.did}" ${tab==o.did?"active":""}>${o.dname}</a></li>
                        </ul>                     
                        <ul>
                          
                             <c:forEach items="${listC}" var= "o">
                                 <li data-filter=".oranges"><a href="category?did=${o.did}" ${tab==o.did?"active":""} style="color: black; text-transform: lowercase; font-weight:600 ">${o.dname}</a></li>
                             </c:forEach>
                        </ul>
                           
                    </div>
                    <div class="countdown">

                        <div class="time days">
                            <div class="number">
                                <span class="time-days">368</span>
                            </div>
                            <div>
                                <span class="label">DAYS</span>
                            </div>

                        </div>
                        <div class="time hours">
                            <div class="number">
                                <span class="time-hours">04</span>
                            </div>
                            <div>
                                <span class="label">HOURS</span>
                            </div>


                        </div>
                        <div class="time mins">
                            <div class="number">
                                <span class="time-mins">21</span>
                            </div>
                            <div>
                                <span class="label">MINS</span>
                            </div>


                        </div>
                        <div class="time secs">
                            <div class="number">
                                <span class="time-secs">12</span>
                            </div>
                            <div>
                                <span class="label">SECS</span>
                            </div>


                        </div>
                    </div>
                </div>
            </div>
            <div class="row featured__filter">
                <c:forEach items = "${listP}" var = "p">
                <div class="col-lg-3 col-md-4 col-sm-6 mix oranges fresh-meat">
                    <div class="featured__item">
                        <div class="featured__item__pic set-bg" data-setbg="${p.imsges}">
                            <ul class="featured__item__pic__hover">
                                <li><a href="#"><i class="fa fa-heart"></i></a></li>
                                <li><a href="#"><i class="fa fa-retweet"></i></a></li>
                                <li><a href="buyc?id=${p.id}"><i class="fa fa-shopping-cart"></i></a></li>
                            </ul>
                        </div>
                        <div class="featured__item__text">
                            <h6><a href="detail?id=${p.id}">${p.name}</a></h6>
                            <h5>${p.price}$</h5>
                        </div>
                    </div>
                </div>
               </c:forEach>
            </div>
        </div>
    </section>
